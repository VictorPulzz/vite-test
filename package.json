  "scripts": {
    "start": "webpack serve --config webpack-config/webpack.dev.js --progress",
    "build": "mkdir -p build && webpack --config webpack-config/webpack.prod.js --progress",
    "prettier": "prettier --write src/**/*.{ts,tsx,scss,json,css,pcss,js,graphql}",
    "lint": "eslint --quiet src/**/*.{ts,tsx}",
    "lint:fix": "eslint src/**/*.{ts,tsx} --fix",
    "check-types": "tsc -p tsconfig.json",
    "prepare": "husky install",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "gql:codegen": "DOTENV_CONFIG_PATH=\"./.env\" graphql-codegen --require dotenv/config --config codegen.yml",
    "gql:codegen:download": "DOTENV_CONFIG_PATH=\"./.env\" graphql-codegen --require dotenv/config --config codegen.download.yml"
  },
